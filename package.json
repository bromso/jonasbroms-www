{
	"name": "jonasbroms-www",
	"version": "0.0.0",
	"description": "Jonas Bröms official website, using the Odduse Monorepo Architecture & Structure built on NodeJS, Turborepo, Yarn, Workspaces, React, Typescript, NextJS, Storybook & Material UI (MUI)",
	"license": "Closed source",
	"private": true,
	"author": {
		"email": "jonasbroms@icloud.com",
		"name": "Jonas Bröms",
		"url": "https://jonasbroms.com"
	},
	"maintainers": [
		{
			"name": "Jonas Bröms",
			"email": "jonasbroms@icloud.com"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/bromso/jonasbroms-www.git"
	},
	"bugs": {
		"url": "https://github.com/bromso/jonasbroms-www/issues"
	},
	"workspaces": [
		"apps/*",
		"packages/*",
		"tools/*",
		"utils/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"www:dev": "turbo run dev --filter=./apps/www",
		"www:build": "turbo run build --filter=./apps/www",
		"docs:dev": "turbo run dev --filter=./apps/docs",
		"docs:build": "turbo run build --filter=./apps/docs",
		"blog:dev": "turbo run dev --filter=./apps/blog",
		"blog:build": "turbo run build --filter=./apps/blog",
		"jobs:dev": "turbo run dev --filter=./apps/jobs",
		"jobs:build": "turbo run build --filter=./apps/jobs",
		"help:dev": "turbo run dev --filter=./apps/help",
		"help:build": "turbo run build --filter=./apps/help",
		"legal:dev": "turbo run dev --filter=./apps/legal",
		"legal:build": "turbo run build --filter=./apps/legal",
		"wiki:dev": "turbo run dev --filter=./apps/wiki",
		"wiki:build": "turbo run build --filter=./apps/wiki",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"eslint-config-custom": "*",
		"prettier": "latest",
		"turbo": "latest"
	},
	"engines": {
    "node": ">=16.0.0"
  },
	"packageManager": "yarn@1.22.19",
	"keywords": [
		"architecture",
		"structure",
		"yarn",
		"workspaces",
		"turborepo",
		"prettier",
		"eslint",
		"vscode",
		"nvm",
		"node version manager",
		"visual code"
	]
}
